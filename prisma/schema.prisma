generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../database.db"
}

model Client {
  id             Int        @id @default(autoincrement())
  ip             String
  mac            String?    @unique
  hostname       String?    @unique
  alias          String     @default("")
  screenshotPath String     @default("")
  bookmarks      Bookmark[]

  @@map("client")
}

model Bookmark {
  id      Int      @id @default(autoincrement())
  url     String?
  clients Client[]

  @@map("bookmark")
}
