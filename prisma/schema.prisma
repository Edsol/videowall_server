generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../database.db"
}

model Client {
  id               Int                @id @default(autoincrement())
  ip               String
  mac              String
  hostname         String?
  BookmarksClients BookmarksClients[]
}

model Bookmark {
  id               Int                @id @default(autoincrement())
  url              String
  BookmarksClients BookmarksClients[]
}

model BookmarksClients {
  client     Client   @relation(fields: [clientId], references: [id])
  clientId   Int
  bookmark   Bookmark @relation(fields: [bookmarkId], references: [id])
  bookmarkId Int

  @@id([clientId, bookmarkId])
}
